{
  "name": "bondacom/laravel-file-manager",
  "description": "File manager for Laravel.",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.4",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bondacom\\LaravelFileManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bondacom\\LaravelFileManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bondacom\\LaravelFileManager\\Providers\\LaravelFileManagerServiceProvider"
      ],
      "aliases": {
        "Reader": "Bondacom\\LaravelFileManager\\Facades\\Reader",
        "Writer": "Bondacom\\LaravelFileManager\\Facades\\Writer"
      }
    }
  }
}